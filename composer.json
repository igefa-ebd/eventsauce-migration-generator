{
    "name": "andreo/eventsauce-migration-generator",
    "description": "This component generates doctrine migrations per aggregate.",
    "keywords": [
        "eventsauce",
        "components",
        "doctrine-migrations"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Pakula",
            "email": "andrze.pakula@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/migrations": "^3.5",
        "doctrine/sql-formatter": "^1.1",
        "eventsauce/eventsauce": "^3.0",
        "eventsauce/message-repository-table-schema": "^0.4",
        "symfony/console": "^6.2",
        "andreo/eventsauce-snapshotting": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andreo\\EventSauce\\Doctrine\\Migration\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
